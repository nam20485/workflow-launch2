{
	"tools": {
		"autoAccept": "false"
	},
	"context": {
		"fileName": "GEMINI.md"
	},
	"mcpServers": {
		"sequential-thinking": {
			"type": "stdio",
			"command": "npx",
			"args": [
				"-y",
				"@modelcontextprotocol/server-sequential-thinking"
			],
			"version": "0.0.1"
		},
		"memory": {
			"type": "stdio",
			"command": "npx",
			"args": [
				"-y",
				"@modelcontextprotocol/server-memory"
			],
			"version": "0.0.1"
		},
		"github": {
			"command": "npx",
			"args": [
				"-y",
				"@modelcontextprotocol/server-github"
			]
		},
		"filesystem": {
			"command": "npx",
			"args": [
				"-y",
				"@modelcontextprotocol/server-filesystem",
				"C:\\Users\\nmill",
				"."
			]
		},
		"gemini-cli": {
			"command": "npx",
			"args": [
				"-y",
				"gemini-mcp-tool"
			],
			"type": "stdio"
		},
		"notion": {
			"command": "npx",
			"args": [
				"-y",
				"@notionhq/notion-mcp-server"
			],
			"env": {
				"OPENAPI_MCP_HEADERS": {
					"Authorization": "Bearer ${env:NOTION_INTEGRATION_TOKEN}",
					"Notion-Version": "2022-06-28"
				}
			},
			"type": "stdio"
		},
		"context7": {
			"command": "npx",
			"args": [
				"-y",
				"@upstash/context7-mcp@latest"
			],
			"type": "stdio"
		},
		"microsoft-docs": {
			"command": "npx",
			"args": [
				"-y",
				"@modelcontextprotocol/server-microsoft-docs"
			]
		},
		"deepwiki": {
			"url": "https://mcp.deepwiki.com/sse",
			"type": "http"
		},
		"playwright": {
			"command": "npx",
			"args": [
				"@playwright/mcp@latest"
			]
		},
		"puppeteer": {
			"command": "npx",
			"args": [
				"-y",
				"@modelcontextprotocol/server-puppeteer"
			],
			"env": {}
		},
		"web-browser": {
			"command": "npx",
			"args": [
				"-y",
				"@modelcontextprotocol/server-web-browser"
			]
		},
		"Tavily": {
			"command": "npx",
			"args": [
				"-y",
				"@modelcontextprotocol/server-tavily"
			]
		},
		"firebase": {
			"command": "npx",
			"args": [
				"-y",
				"firebase-tools@latest",
				"experimental:mcp"
			]
		},
		"Tavily Expert": {
			"command": "npx",
			"args": [
				"-y",
				"@modelcontextprotocol/server-tavily"
			]
		},
		"Tavily: Expert": {
			"command": "npx",
			"args": [
				"-y",
				"@modelcontextprotocol/server-tavily"
			]
		}
	},
	"general": {
		"preferredEditor": "vscode"
	},
	"security": {
		"auth": {
			"selectedType": "oauth-personal"
		}
	},
	"ui": {
		"theme": "Default"
	},
	"disabledTools": [
		"web_fetch"
	],
	"inputs": []
